<ul id="newslist"  data-role="listview" data-inset="true" data-icon="arrow-darkblue">
  <% var source = -1, epCount = 0, html = '', extra = '', allHtml = '', img = '';
_.each(news, function(n, i) {
var e = n.News,
klasse = 'news-entry show-all show-recent news-'+n.NewsSource.rank;
if(disabledNews[n.NewsSource.rank]) return true;
if(e.source_id != source) {
source = e.source_id;
epCount = 0;
%>
  <li class="source-news-li news-divider show-all show-recent news-<%=news[i].NewsSource.rank%>" id="<%= n.NewsSource.id%>" > 
  	<a class="source-news-li-a ui-btn ui-btn-icon-right ui-icon-carat-r" href="#news/source/<%= n.NewsSource.id%>"> <%= n.NewsSource.name %> </a> </li>
  <% }
if(epCount < 4) {
epCount++;
%>
  <li<%=extra%> class="<%=klasse%>" id="<%= e.id %>"> <a href="#news/view/<%= e.id %>"><%= e.headline %>
    <p><%= e.description %> <br>
    <span class="news-date"><%= e.DateString %></p> </a> </li>
  <% }
}); %>
</ul>
