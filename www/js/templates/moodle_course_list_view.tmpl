<% _.each(courses, function(course){ %>

    <li class='moodle-course' pageid='moodle-course-content-<%= course.get('id') %>'>
      <a href="#study/moodle" data-role="button" data-icon="arrow-darkblue" data-transition="slidefade">
        <h2><%= course.get('fullname') %></h2>
      </a>
    </li>

<%});%>
